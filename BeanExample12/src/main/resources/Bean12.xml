<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:annotation-config />

	<bean id="manager" class="com.test.util.Manager" />

	<bean id="carName" class="com.test.util.CarName">
		<property name="car" value="Bmw" />
		<property name="model" value="323i" />
		<property name="year" value="2011" />
	</bean>
	<bean id="carName1" class="com.test.util.CarName">
		<property name="car" value="Benz" />
		<property name="model" value="s300" />
		<property name="year" value="2017" />

	</bean>
</beans>